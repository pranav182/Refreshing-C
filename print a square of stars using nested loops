/* Create a program that displays on the screen a square of n x n stars, with the integer n given 
as an input. */


#include <stdio.h>
int main(void)
{
    int stars;
    scanf("%d",&stars);
    int i,j;
    for(i=0;i<stars;i++)
    {
        for(j=0;j<stars;j++)
        {
            printf("*");
        }
        printf("\n");
    }
    return 0;
}
